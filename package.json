{
    "name": "code-extension",
    "displayName": "code-extension",
    "publisher": "damandhillons",
    "description": "VSCode extension for getting notifications using Apollo GraphQL Subscriptions",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onCommand:code-extension.deregister-subscription",
        "onCommand:code-extension.register-subscription",
        "onCommand:code-extension.runTask"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [{
                "command": "code-extension.deregister-subscription",
                "title": "Demo: Unsubscribe from Book Added"
            },
            {
                "command": "code-extension.register-subscription",
                "title": "Demo: Subscribe to Book Added"
            },
            {
                "command": "code-extension.runTask",
                "title": "Demo: Run Task"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/uuid": "^8.3.4",
        "@types/vscode": "^1.73.0",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.26.0",
        "glob": "^8.0.3",
        "html-loader": "^4.2.0",
        "mocha": "^10.1.0",
        "ts-loader": "^9.4.1",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "@apollo/client": "^3.7.1",
        "axios": "^1.1.3",
        "cross-fetch": "^3.1.5",
        "graphql": "^16.6.0",
        "graphql-ws": "^5.11.2",
        "node-fetch": "^3.2.10",
        "uuid": "^9.0.0",
        "ws": "^8.11.0"
    }
}